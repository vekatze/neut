  print-rune(`ã‚`);
  print-rune(`Ã©`);
  print-rune(`Ø¨`);
  print-rune(`ğ ®·`);
  print-rune(`ğ €‹`);
  print-rune(`ğŸ’«`);
  match `a` {
  | `x` =>
    print-rune(`x`)
  | `a` =>
    print-rune(`a`)
  | _ =>
    print("other\n")
  };
  let p: except(pair(rune, type), type) = Error(Pair(`x`, type)) in
  match p {
  | OK(_) =>
    print("right\n")
  | Error(Pair(`a`, _)) =>
    print("pair-a\n")
  | Error(Pair(`x`, _)) =>
    print("pair-x\n")
  | Error(Pair(_, _)) =>
    print("pair-other\n")
  }
