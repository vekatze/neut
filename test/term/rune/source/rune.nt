  match `a` {
  | `x` =>
    print-rune(`x`)
  | `a` =>
    print-rune(`a`)
  | _ =>
    print("other\n")
  };
  let p: either(pair(rune, type), type) = Left(Pair(`x`, type));
  match p {
  | Right(_) =>
    print("right\n")
  | Left(Pair(`a`, _)) =>
    print("pair-a\n")
  | Left(Pair(`x`, _)) =>
    print("pair-x\n")
  | Left(Pair(_, _)) =>
    print("pair-other\n")
  }
}

