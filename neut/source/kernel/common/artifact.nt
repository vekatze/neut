import {
  time.time {time},
}

data artifact-time {
| Artifact-Time(
    cache-time: ?time,
    llvm-time: ?time,
    object-time: ?time,
  )
}

define empty-artifact-time: artifact-time {
  Artifact-Time(none, none, none)
}

define artifact-time-from-time(!t: time): artifact-time {
  Artifact-Time(Right(!t), Right(!t), Right(!t))
}
