import {
  core.dictionary {dictionary},
  core.ref {make-ref, ref},
  this.kernel.common.optimizable-data {optimizable-data},
  this.language.common.definite-description {definite-description},
}

data optimizable-data-kit {
| _Optimizable-Data-Kit(
    opt-data-map-ref: ref(dictionary(definite-description, optimizable-data)),
  )
}

define make-optimizable-data-kit(): optimizable-data-kit {
  let opt-data-map-ref = make-ref(box {core.dictionary.empty});
  _Optimizable-Data-Kit of {opt-data-map-ref}
}
