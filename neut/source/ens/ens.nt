import {
  this.logger.hint {hint},
  this.syntax-tree.block {block},
  this.syntax-tree.comment {comment},
  this.syntax-tree.series {series},
}

data ens-f(a: type) {
| E-Int(a, int)
| E-Float(a, int)
| E-Bool(a, bool)
| E-String(a, text)
| E-List(a, series(ens-f(a)))
| E-Dictionary(a, series(pair(text, ens-f(a))))
}

inline ens: type {
  ens-f(hint)
}

data top-ens {
| Top-Ens(ens, comment)
}

inline full-ens {
  block(ens)
}
