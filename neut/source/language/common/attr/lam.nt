import {
  this.language.common.lam-kind {Normal, lam-kind-f},
}

inline _ID: type {
  int
}

data attr-lam(a) {
| Attr-Lam(
    lam-kind: lam-kind-f(a),
    identity: _ID,
  )
}

define normal<a>(i: _ID, cod-type: a): attr-lam(a) {
  Attr-Lam of {
    lam-kind := Normal(none, cod-type),
    identity := i,
  }
}
