data conv-op {
| Trunc
| Zext
| Sext
| Fptrunc
| Fpext
| Fptoui
| Fptosi
| Uitofp
| Sitofp
}

define show-conv-op(op: conv-op): &text {
  match op {
  | Trunc =>
    "trunc"
  | Zext =>
    "zext"
  | Sext =>
    "sext"
  | Fptrunc =>
    "fptrunc"
  | Fpext =>
    "fpext"
  | Fptoui =>
    "fptoui"
  | Fptosi =>
    "fptosi"
  | Uitofp =>
    "uitofp"
  | Sitofp =>
    "sitofp"
  }
}
